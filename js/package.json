  "author": "Hai Nguyen <hainm.comp@gmail.com>, Alexander Rose <alexander.rose@weirdbyte.de>",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arose/nglview.git"
  },
  "bugs": {
    "url": "https://github.com/arose/nglview/issues"
  },
  "files": [
    "dist",
    "src",
    "lib"
  ],
  "keywords": [
    "molecular graphics",
    "molecular structure",
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "science"
  ],
  "scripts": {
    "lint": "eslint src test",
    "prepublish": "tsc && webpack",
    "ts": "tsc",
    "test": "mocha",
    "build": "yarn run build:lib",
    "build:labextension": "cd jupyter_archive && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:all": "yarn run build:labextension",
    "clean": "yarn run clean:lib",
    "clean:lib": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyter_archive/labextension",
    "clean:all": "yarn run clean:lib && yarn run clean:labextension",
    "prepare": "yarn run clean && yarn run build",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "@types/chai": "^4.1.4",
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^2.2.48",
    "@types/requirejs": "^2.1.31",
    "typescript": "~3.0.0",
    "babel-eslint": "^7.0.0",
    "babel-register": "^6.11.6",
    "css-loader": "^0.23.1",
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.7.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "ngl": "2.0.0-dev.36",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "underscore": "^1.8.3",
    "ngl": "2.0.0-dev.36",
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3"
  },
  "jupyterlab": {
    "extension": "lib/jupyterlab-plugin"
  },
  "homepage": "https://github.com/arose/nglview#readme",
  "directories": {
    "test": "test"
  }
